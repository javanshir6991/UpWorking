{
  "kind": "collectionType",
  "collectionName": "applications",
  "info": {
    "singularName": "application",
    "pluralName": "applications",
    "displayName": "Application"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "Email": {
      "type": "email"
    },
    "Phone": {
      "type": "biginteger"
    },
    "ApplicationStatus": {
      "type": "enumeration",
      "enum": [
        "Pending",
        "Accepted",
        "Rejected"
      ]
    },
    "job": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::job.job",
      "inversedBy": "applications"
    }
  }
}
